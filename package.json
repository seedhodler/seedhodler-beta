{
  "name": "seedhodler-next",
  "version": "1.0.0",
  "description": "Shamir Secret Sharing built for BIP39 Mnemonics implementing SLIP39",
  "author": "seedhodler",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "precommit": "yarn lint",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "offline": "nuxt generate && node server.js",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "bip32": "^2.0.4",
    "bip39": "^3.0.2",
    "create-hash": "^1.2.0",
    "nuxt": "^2.11.0",
    "nuxt-buefy": "^0.3.24",
    "push-dir": "^0.4.1",
    "slip39": "^0.1.6",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": ">=14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": ">=23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-nuxt": ">=0.5.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": ">=4.0.1",
    "eslint-plugin-vue": "^6.2.1",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.1",
    "sass-loader": "^8.0.2"
  }
}
