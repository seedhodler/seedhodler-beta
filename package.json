{
  "name": "seedhodler-next",
  "version": "1.0.0",
  "description": "Shamir Secret Sharing built for BIP39 Mnemonics. Alternative implementation to https://github.com/iancoleman/shamir39",
  "author": "seedhodler",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "precommit": "yarn lint",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "bip32": "^2.0.4",
    "bip39": "^3.0.2",
    "nuxt": "^2.0.0",
    "nuxt-buefy": "^0.3.2",
    "push-dir": "^0.4.1",
    "slip39": "^0.1.5-dev.1",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.18.2",
    "sass-loader": "^7.2.0"
  }
}
